import pip
import plotly
import plotly.plotly as py
import plotly.graph_objs as go

fig = {
    'data': [{'labels': ['Internet Banking', 'Mobile Banking'],
              'values': [8780, 187],
              'type': 'pie'}],
    'layout': {'title': 'Internet Banking&Mobile Banking comparing 2011'}
}

url = py.plot(fig, filename='Pie Chart 2011')
